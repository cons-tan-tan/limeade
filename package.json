{
  "name": "limeade",
  "description": "LIMEでの日報申請を少しだけ楽にする",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=24.4.1",
    "pnpm": ">=10.13.1"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "check": "pnpm biome check --write && tsc --noEmit",
    "prepare": "wxt prepare && pnpm lefthook install"
  },
  "dependencies": {
    "@marcbachmann/cel-js": "^1.0.5",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "lefthook": "^1.12.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  }
}
